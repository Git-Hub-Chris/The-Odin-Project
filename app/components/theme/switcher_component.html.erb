<%= link_to(
  themes_path(theme: other_theme.name),
  class: "odin-dark-nav-item #{yass(link: type)}",
  data: {
    turbo: "true",
    'turbo-method' => :put,
    "turbo-frame" => "_top",
  }
) do %>
  <%= inline_svg_tag "icons/#{current_theme.icon}.svg", class: "odin-dark-nav-item #{yass(icon: type)}", aria: true, title: 'theme icon' %>
  <%= text unless icon_only? %>
<% end %>

