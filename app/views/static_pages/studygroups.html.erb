      <!-- Tatiana's web dev 101 track starting 4/20/14 -->
      <div class="tab-pane fade" id="track3">
      <!-- app/views/static_pages/_sg_wd101_a.html.erb -->
        <%= render "sg_wd101_b" %>
      </div> 

      <!-- Brian's ruby track starting 4/28/14 -->
      <div class="tab-pane fade" id="track4">
      <!-- app/views/static_pages/_sg_ruby.html.erb -->
        <%= render "sg_ruby" %>
      </div> 


      <!-- Starting your own study group -->
      <div class="tab-pane fade" id="track5">
        <!-- app/views/static_pages/_sg_starting_one.html.erb -->
